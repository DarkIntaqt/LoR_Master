{
  "name": "lor-master-tracker-ui",
  "productName": "LoR Master Tracker",
  "version": "0.9.8",
  "private": true,
  "main": "app.js",
  "scripts": {
    "dev": "vue-cli-service build && electron app.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "deploy.sh",
    "start": "electron app.js",
    "package": "electron-packager ./ LoRMasterTracker --out ./app --app-version ${npm_package_version} --overwrite --icon=image.ico"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "runeterra": "^2.0.0",
    "tasklist": "^4.0.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0-rc.2",
    "zeromq": "^6.0.0-beta.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^11.2.3",
    "electron-packager": "^15.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
